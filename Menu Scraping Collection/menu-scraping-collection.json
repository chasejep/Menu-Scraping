								"exec": [
									"// Get the dutchie_remainingProductCount",
									"let remainingProductCount = pm.collectionVariables.get('dutchie_remainingProductCount');",
									"if (remainingProductCount <= 0) {",
									"    throw new Error(\"No more products to fetch. Stopping request execution.\");",
									"}",
									"",
									"// Get the dutchie_currentPage",
									"let currentPage = pm.collectionVariables.get('dutchie_currentPage');",
									"",
									"// If currentPage is not set (undefined, null, or blank), set it to 0",
									"if (!currentPage) {",
									"    currentPage = 0;",
									"} else {",
									"    currentPage = parseInt(currentPage, 10); // Ensure it's an integer",
									"}",
									"",
									"pm.collectionVariables.set('dutchie_currentPage', currentPage);",
									""
								],
