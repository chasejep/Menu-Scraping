				{
					"name": "Dutchie Menu Products",
					"event": [
						{
							"listen": "test",
							"script": {
								"id": "6551c62e-aea3-4151-a2de-fffc1a6b5b1e",
								"exec": [
									"// Update dutchie_remainingProductCount",
									"let remainingProductCount = pm.collectionVariables.get('dutchie_remainingProductCount');",
									"let newRemainingProductCount = remainingProductCount - 100;",
									"newRemainingProductCount = newRemainingProductCount < 0 ? 0 : newRemainingProductCount;",
									"pm.collectionVariables.set('dutchie_remainingProductCount', newRemainingProductCount);",
									"",
									"// Add response to dutchie_collectionResponses variable",
									"let responses = pm.collectionVariables.get('dutchie_collectionResponses');",
									"if(responses) {",
									"  responses = JSON.parse(responses);",
									"} else {",
									"  responses = [];",
									"}",
									"responses.push(pm.response.json());",
									"pm.collectionVariables.set('dutchie_collectionResponses', JSON.stringify(responses));",
									"",
									" // Increment the page only after adding the response",
									"    let currentPage = pm.collectionVariables.get('dutchie_currentPage');",
									"    currentPage++;",
									"    pm.collectionVariables.set('dutchie_currentPage', currentPage);",
									"",
									"// Set next request",
									"if (newRemainingProductCount > 0) {",
									"    pm.execution.setNextRequest(\"Dutchie Menu Products\");",
									"} else {",
									"    console.log(\"All products have been fetched.\");",
									"    pm.execution.setNextRequest(null); // Stop further requests",
									"}"
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
